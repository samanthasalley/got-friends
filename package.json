{
    "name": "got-friends",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=production node ./server/server.js",
        "dev": "NODE_ENV=development node ./server/server.js",
        "build:dev": "NODE_ENV=development webpack --config ./webpack/dev.config.js",
        "build:prod": "NODE_ENV=production webpack --config ./webpack/prod.config.js",
        "dev:hot": "NODE_ENV=development-hot nodemon ./server/server.js & NODE_ENV=development webpack-dev-server --config ./webpack/dev.config.js",
        "test": "NODE_ENV=test jest",
        "test-coverage": "NODE_ENV=test jest --coverage",
        "test-watch": "NODE_ENV=test jest --watch",
        "test-updateSnapshot": "NODE_ENV=development jest --updateSnapshot",
        "add-dep": "bash ./scripts/npm-docker/add-dep.sh",
        "update-dep": "bash ./scripts/npm-docker/update-dep.sh",
        "image-remove": "docker image rm samanthasalley/<dependencies-image-name> --force",
        "image-build": "docker build -t samanthasalley/<dependencies-image-name> -f Dockerfile-dev .",
        "docker-build:dev": "docker-compose run --rm --service-ports build-dev",
        "docker-dev": "docker-compose -f docker-comp-dev.yml up",
        "docker-test": "docker-compose -f docker-comp-test.yml up --abort-on-container-exit",
        "docker-remove-all:danger": "docker rm $(docker ps -a -q -f 'name=<name-partial>') --force && docker rmi $(docker images samanthasalley/<name-partial>* -q) --force && docker volume rm $(docker volume ls -q -f name=<name-partial>) --force",
        "docker-stop": "docker stop $(docker ps -a -q --filter status=running)",
        "docker-dev:hot": "docker-compose -f docker-comp-dev.yml -f docker-comp-dev-hot.yml up"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.3",
        "@fortawesome/fontawesome-free-regular": "^5.0.6",
        "@fortawesome/fontawesome-free-solid": "^5.0.6",
        "@fortawesome/react-fontawesome": "0.0.17",
        "alertify.js": "^1.0.12",
        "aws-sdk": "^2.241.1",
        "babel-jest": "^22.4.1",
        "babel-polyfill": "^6.26.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "~1.18.2",
        "config": "^1.29.1",
        "connect-pg-simple": "^4.2.1",
        "cookie-parser": "~1.4.3",
        "db-migrate-pg": "^0.3.1",
        "debug": "~2.6.9",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "express": "~4.15.5",
        "express-session": "^1.15.6",
        "express-validator": "^4.3.0",
        "gsap": "^1.20.6",
        "immutability-helper": "^2.6.4",
        "isomorphic-fetch": "^2.2.1",
        "moment": "^2.22.2",
        "moment-timezone": "^0.5.14",
        "morgan": "~1.9.0",
        "node-fetch": "^2.2.0",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "pg": "^7.4.1",
        "prop-types": "^15.6.2",
        "query-string": "^6.1.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-ga": "^2.5.3",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "react-test-renderer": "^16.3.2",
        "react-virtualized": "^9.18.5",
        "redux": "^3.7.2",
        "redux-form": "^7.2.0",
        "redux-mock-store": "^1.5.1",
        "redux-thunk": "^2.2.0",
        "req-flash": "0.0.3",
        "yargs": "^11.0.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.8",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.6",
        "html-webpack-plugin": "^2.30.1",
        "jest": "^22.0.4",
        "node-mocks-http": "^1.6.6",
        "node-sass": "^4.7.2",
        "nodemon": "^1.14.9",
        "postcss-loader": "^2.1.5",
        "raw-loader": "^0.5.1",
        "react-hot-loader": "^3.1.3",
        "react-svg-loader": "^2.1.0",
        "redux-devtools-extension": "^2.13.2",
        "resolve-url-loader": "^2.3.0",
        "sass-loader": "^6.0.6",
        "sinon": "^4.1.6",
        "style-loader": "^0.19.1",
        "uglifyjs-webpack-plugin": "^1.1.8",
        "url-loader": "^0.6.2",
        "webpack": "^3.10.0",
        "webpack-dev-middleware": "^1.12.2",
        "webpack-dev-server": "^2.10.0",
        "webpack-hot-middleware": "^2.21.0"
    },
    "jest": {
        "testEnvironment": "jest-environment-node",
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/node_modules/**",
            "!**/webpack/**",
            "!**/coverage/**"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/client/assets/",
            "<rootDir>/node_modules/"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/client/assets/",
            "<rootDir>/__tests__/__mocks__/",
            "<rootDir>/server/views/"
        ],
        "moduleNameMapper": {
            "@root(.*)$": "<rootDir>/$1",
            "@client(.*)$": "<rootDir>/client/$1",
            "@server(.*)$": "<rootDir>/server/$1",
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/__mocks__/fileMock.js"
        }
    },
    "main": "server/server.js",
    "author": "",
    "license": "ISC",
    "description": ""
}